{
  "name": "pokedex",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "cy:open-e2e": "cypress open --e2e --browser edge",
    "cy:open-unit": "cypress open --component --browser edge",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy": "start-server-and-test start http-get://localhost:3000 cy:open-e2e"
  },
  "dependencies": {
    "axios": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reactflow": "^11.7.4"
  },
  "devDependencies": {
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/axios": "0.14.0",
    "@types/faker": "5.5.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/ui": "0.32.2",
    "cypress": "^12.17.3",
    "eslint": "8.38.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "faker": "5.5.3",
    "jsdom": "21.1.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "5.0.2",
    "vite": "4.3.9",
    "vitest": "0.31.0"
  }
}